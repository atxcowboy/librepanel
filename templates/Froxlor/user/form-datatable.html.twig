{% extends "LibrePanel/user/form.html.twig" %}

{% block content %}
	{% if tabledata.table.tr is not empty and tabledata.table.tr is iterable %}
		<div class="row mb-2">
			{% set type = 'warning' %}
			{% set alert_msg = lng('error.customerhasongoingexportjob') %}
			{% include 'LibrePanel/misc/alertbox.html.twig' %}
		</div>
		{% import "LibrePanel/table/table.html.twig" as table %}
		{{ table.table(tabledata) }}
	{% endif %}

	{{ parent() }}

{% endblock %}
